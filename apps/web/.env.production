# PerpMate Web Production Environment Variables
# Copy this file to .env.local and fill in your actual values

# Database Configuration (same as bot)
DATABASE_URL=postgresql://postgres:pass@localhost:5432/perpmate

# Privy Configuration (public)
NEXT_PUBLIC_PRIVY_APP_ID=

# API Configuration
NEXT_PUBLIC_API_URL=

# Hyperliquid Configuration (public)
NEXT_PUBLIC_HYPEREVM_RPC_URL=https://api.hyperliquid.xyz

# WebSocket Configuration (public)
NEXT_PUBLIC_WS_URL=


# Environment
NEXT_PUBLIC_ENVIRONMENT=production